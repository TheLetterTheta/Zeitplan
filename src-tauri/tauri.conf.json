{
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:8080"
  },
  "ctx": {},
  "tauri": {
    "embeddedServer": {
      "active": true
    },
    "allowlist": {
      "event": true,
      "notification": true
    },
    "bundle": {
      "active": true,
      "identifier": "com.zeitplan.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "GPL-3.0-or-later",
      "category": "Productivity",
      "shortDescription": "A student scheduling app",
      "longDescription": "A scheduling app built around a private instructor's specifications. Searches for availability across multiple users and multiple required meetings simultaneoulsy, to allow for a better fit.",
      "deb": {
        "depends": []
      },
      "osx": {
        "frameworks": [],
        "minimumSystemVersion": ""
      },
      "exceptionDomain": ""
    },
    "window": {
      "title": "Zeitplan",
      "width": 992,
      "height": 1145
    },
    "security": {
      "csp": "default-src blob: data: file: filesystem: ws: http: https: 'unsafe-eval' 'unsafe-inline'"
    },
    "inliner": {
      "active": true
    }
  }
}
